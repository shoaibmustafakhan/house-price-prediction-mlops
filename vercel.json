{
    "version": 2,
    "builds": [
      { "src": "app.py", "use": "@vercel/python" },
      { "src": "public/index.html", "use": "@vercel/static-build", "config": { "distDir": "public" } }
    ],
    "routes": [
      { "src": "/predict", "dest": "/app.py" },
      { "src": "/(.*)", "dest": "/public/index.html" }
    ]
}
