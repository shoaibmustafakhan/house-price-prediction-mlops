{
    "version": 2,
    "builds": [
      { "src": "app.py", "use": "@vercel/python", "config": { "runtime": "python3.9" } }
    ],
    "routes": [
      { "src": "/predict", "dest": "/app.py" },
      { "src": "/(.*)", "dest": "/app.py" }
    ],
    "hooks": {
      "build": "pip install --upgrade pip"
    }
}
